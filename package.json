{
  "name": "wire.keeper.ts2",
  "version": "1.0.0",
  "description": "client app for muppet",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && electron .",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrierenZk/wire.keeper.ts2.git"
  },
  "author": "FrierenZk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FrierenZk/wire.keeper.ts2/issues"
  },
  "homepage": "https://github.com/FrierenZk/wire.keeper.ts2#readme",
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.1",
    "socket.io-client": "^2.4.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/bootstrap": "^5.1.6",
    "@types/socket.io-client": "^1.4.35",
    "electron": "^11.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "typescript": "^4.5.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": [
          "node_modules/@*",
          "node_modules/_@*",
          ".git",
          "src",
          ".idea",
          "tsconfig.json",
          ".gitignore",
          "data/config"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wire.keeper.ts"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
